#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b,c,d;
	    cin>>a>>b>>c>>d;
	    if((b+c)==a || (c+d)==a || (b+d)==a || (d+b+c==a)){ 
	        cout<<"YES"<<endl;
	    }
	    else if(a==b || a==c || a==d){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
