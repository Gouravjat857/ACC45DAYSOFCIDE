#include <iostream>
using namespace std;
int main() {
    int t,a,b,c;
    cin>>t;
    while(t--){
        cin>>a>>b>>c;
      if((a+c)==b||(a+b)==c||(b+c)==a){
       cout<<"yes"<<endl;
       }
        else{
            cout<<"no"<<endl;
        }
        
    }
	// your code goes here
	return 0;
}
