include <iostream>
using namespace std;

int main() {
	// your code goes here
	  int a,b,x;
     cin >>a >>b >>x;
     int arr[a];
     string s = "No";

          for(int i=0; i<a; i++){
              cin >>arr[i];
              if (arr[i]+x >=b) s = "Yes";


          }
             cout<< s <<endl;

	return 0;
}
