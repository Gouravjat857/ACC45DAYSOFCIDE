#include<iostream>
using namespace std;
int main(){
    int t,x,y;
    cout<<"enter the no. of test cases\n";
    cin>>t;
    while(t--){
        cout<<"litres of water present in geaser\n";
        cin>>x;
        cout<<"litres of water present in bucket\n";
        cin>>y;
        if(x>=y*2){
        cout<<"peoples can bath\n"<<x/(y*2)<<endl;
        }
        else if(x<y*2){
            cout<<"peoples cannot bath\n";
        }
    }
}
