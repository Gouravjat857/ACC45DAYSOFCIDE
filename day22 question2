#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	double x,y;
	char c;
	cin>>x;
	cin>>y;
	cin>>c;
	if(c=='+'){
	    cout<<x+y<<endl;
	}
	else if(c=='-'){
	    cout<<x-y<<endl;
	}
	else if(c=='*'){
	    cout<<x*y<<endl;
	}
	else if(c=='/'){
	    cout<<fixed<<setprecision(7)<<double(x)/y<<endl;
	}
	return 0;
}
