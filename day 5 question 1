#include <iostream>
using namespace std;

int main() {
    int t,x;
    cout<<" no. of test cases\n";
    cin>>t;
    while(t--){
      cout<<"battery percentage\n";
      cin>>x;
      if(x==15){
      cout<<"yes\n";
      }
      else if(x<15){
          cout<<"yes\n";
      }
      else{
          cout<<"no\n";
      }
    }
	return 0;
}
